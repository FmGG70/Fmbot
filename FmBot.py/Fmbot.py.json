{
    "type": "discord",
    "display": "discord.py Bot",
    "name": "discord-py-bot",
    "install": [
        {
            "type": "command",
            "commands": [
                "apt update -y && apt install -y unzip curl",
                "curl -L \"https://github.com/FmGGfr/PufferPanel_Template_FmGG/archive/refs/heads/main.zip\" -o source.zip",
                "unzip -o source.zip -d src",
                "rm source.zip",
                "FOLDER=$(ls src) && mv src/$FOLDER/Fmbot.py/* . && rm -rf src",
                "python3 -m venv venv",
                "./venv/bin/pip install --upgrade pip",
                "./venv/bin/pip install -r requirements.txt"
            ]
        }
    ],
    "run": {
        "command": "./venv/bin/python ${bot-py-file}",
        "stopCode": 9,
        "stdin": {
            "type": "stdin"
        }
    },
    "data": {
        "bot-py-file": {
            "type": "string",
            "value": "bot.py",
            "display": "Main bot file",
            "desc": "The main bot file.",
            "required": true,
            "userEdit": true
        }
    },
    "environment": {
        "type": "host"
    },
    "supportedEnvironments": [
        { "type": "host" },
        { "type": "docker", "image": "python" }
    ],
    "requirements": {
        "binaries": ["python3"]
    }
}
